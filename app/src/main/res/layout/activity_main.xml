<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Profile.MainActivity">


   <Button
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_centerInParent="true"
       android:text="go to menu"
       android:id="@+id/goMenu"
      />
   <Button
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_below="@+id/goMenu"
       android:text="go to detail"
       android:layout_centerInParent="true"
       android:id="@+id/goDetail"
       />
   <Button
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_centerInParent="true"
       android:layout_below="@+id/goDetail"
       android:text="go to add"
       android:id="@+id/goAddProd"
       />
   <Button
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_centerInParent="true"
       android:layout_below="@+id/goAddProd"
       android:text="test"
       android:id="@+id/goTest"
       />
   <Button
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_centerInParent="true"
       android:layout_below="@+id/goTest"
       android:text="ListCat"
       android:id="@+id/goListCat"
       />
   <Button
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_centerInParent="true"
       android:layout_below="@+id/goListCat"
       android:text="Calendrier"
       android:id="@+id/goCalendar"
       />
   <Button
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_centerInParent="true"
       android:layout_below="@+id/goCalendar"
       android:text="Reclamations"
       android:id="@+id/goReclamation"
       />
   <Button
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_centerInParent="true"
       android:layout_below="@+id/goReclamation"
       android:text="multi select"
       android:id="@+id/SM"
       />

</RelativeLayout>